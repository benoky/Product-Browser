<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SIGNUP_001Controller">
   <children>
      <Pane prefHeight="360.0" prefWidth="450.0">
         <children>
            <TextField fx:id="signUp_id" layoutX="75.0" layoutY="50.0" prefHeight="25.0" prefWidth="300.0" promptText="ID" />
            <PasswordField fx:id="signUp_pw" layoutX="75.0" layoutY="100.0" prefHeight="25.0" prefWidth="300.0" promptText="PW" />
            <TextField fx:id="signUp_name" layoutX="75.0" layoutY="150.0" prefHeight="25.0" prefWidth="300.0" promptText="사용자명" />
            <Button fx:id="checkID_btn" layoutX="75.0" layoutY="280.0" mnemonicParsing="false" onAction="#CHECK_ID_EVENT" prefHeight="40.0" prefWidth="90.0" text="아이디 확인" />
            <Button fx:id="SIGNUP_btn" layoutX="285.0" layoutY="280.0" mnemonicParsing="false" onAction="#SIGN_UP_END_EVENT" prefHeight="40.0" prefWidth="90.0" text="회원가입" />
            <TextField fx:id="signUp_email" layoutX="75.0" layoutY="200.0" prefHeight="25.0" prefWidth="300.0" promptText="이메일" />
         </children>
      </Pane>
   </children>
</AnchorPane>
